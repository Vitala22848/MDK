<Window x:Class="AppLibrary.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppLibrary"
        mc:Ignorable="d"
        Title="Библиотека" Height="700" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>

        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Center">
            <TextBlock Text="Практическая работа Студента группы ИСП 21.2А Шапедько Виталия"/>
        </StackPanel>

        <Frame Grid.Row="1"
               Name="frmMain"
               ContentRendered="frmMain_ContentRendered"
               NavigationUIVisibility="Hidden">
            
        </Frame>
    </Grid>
</Window>
